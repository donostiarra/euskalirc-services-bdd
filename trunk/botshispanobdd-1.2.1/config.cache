PROGRAM="services"
BINDEST="/home/donostiarra/ircd/servicios"
DATDEST="/home/donostiarra/ircd/servicios"

INSTALL="install -g donostiarra -m 750"
CP_ALL="/bin/cp -dpr"

RUNGROUP="donostiarra"
UMASK=007
NETWORK_DOMAIN="euskalirc.tk"
IRCTYPE=30
IRCTYPE_DEF="IRC_UNDERNET"
IRCTYPE_DEF2="IRC_UNDERNET_P09"
IRCTYPE_DEF3=""
ENCRYPTION=""
REG_NICK_MAIL="REG_NICK_MAIL"
SENDMAIL="SENDMAIL"

CC="gcc"
CC_FLAGS="-O9 -fomit-frame-pointer -ffast-math -fexpensive-optimizations"
CC_LFLAGS=""
CC_LIBS="-lnsl -lresolv"

TYPE_INT16=short
TYPE_INT32=int

HAVE_STRINGS_H=1
HAVE_SYS_SELECT_H=1
HAVE_SYS_SYSPROTO_H=0

HAVE_STRERROR=1
HAVE_SYS_ERRLIST=0

HAVE_SNPRINTF=0
BAD_SNPRINTF=2
HAVE_STRICMP=0
HAVE_STRCASECMP=1
HAVE_STRDUP=1
HAVE_STRSPN=1
HAVE_STRSIGNAL=1
HAVE_GETTIMEOFDAY=1
HAVE_SETGRENT=1
HAVE_UMASK=1
HAVE_FORK=1
HAVE_GETHOSTBYNAME=1
MISSING=""
